a:link {color: #000000; text-decoration: none;}     /* 未访问的链接 */
a:visited {color: #000000; text-decoration: none;}  /* 已访问的链接 */
a:hover {color: #4ed0b6; text-decoration: none;}    /* 当有鼠标悬停在链接上 */
a:active {color: #4ed0b6; text-decoration: none;}

 * {
 	margin: 0;
 	padding: 0;
 	font-family: "Microsoft YaHei"
 }

 body {
 	background-color: #ffffff;
 }

 #main {
 	width: 100%;
 	min-width: 800px;
 	max-width: 1920px;
 }


 #nav {
 	width: 100%;
 	height: 105px;
 	position: relative;
 	background-color: #ffffff;
 	z-index: 1;
 }

 #nav span {
 	display: inline-block;
 }

 #nav .logo {
 	top: 23px;
 	left: 27%;
 	width: 172px;
 	height: 57px;
 	position: absolute;
 	background: url('../images/nav_logo.png') no-repeat;
 	background-size: cover;
 }

 #nav ul {
 	top: 0;
 	right: 23%;
 	font-size: 18px;
 	padding-top: 40px;
 	position: absolute;
 }

 #nav ul li {
 	height: 30px;
 	line-height: 30px;
 	margin-right: 55px;
 	display: inline-block;
 }

 #nav ul li:hover {
	color: #4ed0b6;
	border-bottom: 2px solid #4ed0b6;
 }

 #banner {
 	width: 100%;
 	height: 800px;
 	position: relative;
 	text-align: center;
 	background: url('../images/banner.png') center center no-repeat;
 	background-size: cover;
 }

 #banner img {
 	width: 100%;
 	height: 100%;
 	max-height: 800px;
 }

#banner .text {
	left: 0;
	top: 35.5%;
	color: #fff;
	width: 100%;
	position: absolute;

}

#banner .text span {
	width: 100%;
	display: block;

}

#banner .text .main {
	height: 110px;
	font-size: 110px;
	line-height: 110px;
}

#banner .text .other {
	height: 26px;
	font-size: 26px;
	line-height: 26px;
	margin-top: 40px;
}

#banner .text .btn {
	height: 73px;
	width: 224px;
	margin: 115px auto 0 auto;
	font-size: 26px;
	line-height: 73px;
	border-radius: 8px;
	cursor: pointer;
	border: 2px solid #ffffff;
}

#banner .text .btn a {
	width: 100%;
	height: 100%;
	display: inline-block;
}

#banner .text .btn a:link,
#banner .text .btn a:visited,
#banner .text .btn a:hover,
#banner .text .btn a:active {
	color: #ffffff;
}

